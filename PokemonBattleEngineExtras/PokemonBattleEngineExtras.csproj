<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<OutputType>Exe</OutputType>
		<RootNamespace>Kermalis.PokemonBattleEngineExtras</RootNamespace>
		<Authors>Kermalis</Authors>
		<Copyright>Kermalis</Copyright>
		<RepositoryUrl>https://github.com/Kermalis/PokemonBattleEngine</RepositoryUrl>
		<NoWarn />
		<StartupObject>Kermalis.PokemonBattleEngineExtras.Program</StartupObject>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Dependencies\**" />
		<None Remove="DumpedData\**" />
		<PackageReference Include="System.Drawing.Common" Version="7.0.0" />
		<ProjectReference Include="..\PokemonBattleEngine.DefaultData\PokemonBattleEngine.DefaultData.csproj" />
		<Reference Include="Dependencies\SimpleNARC.dll" />
	</ItemGroup>

</Project>
